{
    "name": "appdezign/lara-base-theme",
    "description": "Lara CMS Base Theme",
	"version": "8.0.0",
    "keywords": ["Lara", "CMS"],
    "license": "proprietary",
    "homepage": "http://firmaq.nl",
    "type": "laravel-library",
    "authors": [
        {
            "name": "Sybrand Hoeksma",
            "email": "s.hoeksma@firmaq.nl"
        }
    ],
    "require": {
		"php": "^8.0",
		"appdezign/lara-cms": "^8.0",
		"appdezign/lara-demo-theme": "^8.0",
		"laravel/framework": "^10.0"
	},
	"autoload": {
		"psr-4": {
			"Laratheme\\Base\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"LaraBaseServiceProvider"
			]
		},
		"installer-paths": {
			"laracms/core": [
				"appdezign/lara-cms"
			],
			"laracms/themes/{$name}": [
				"appdezign/lara-base-theme",
				"appdezign/lara-demo-theme"
			]
		}
	}
}
